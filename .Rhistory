require(TTR)
### Make a full-year dataframe
yearsdf<-as.data.frame(1816:2007)
names(yearsdf)<-"Year"
yearsdf$Year<-as.factor(yearsdf$Year)
### Make a dataframe for # of war entries each year
startyear<-as.data.frame(table(df$StartYear1))
names(startyear)<-c("Year", "Entries")
### Merge dataframe for total war entries in all years
yearsdf<-merge(yearsdf, startyear, by="Year", all.x=TRUE)
yearsdf$Entries<-ifelse(is.na(yearsdf$Entries), 0, yearsdf$Entries)
### Make a dataframe for # of war exits each year
endyear<-as.data.frame(table(df$EndYear1))
names(endyear)<-c("Year", "Exits")
### Merge exits with entries
yearsdf<-merge(yearsdf, endyear, by="Year", all.x=TRUE)
yearsdf$Exits<-ifelse(is.na(yearsdf$Exits), 0, yearsdf$Exits)
yearsdf$Exits<-as.ts(yearsdf$Exits)
yearsdf$Entries<-as.ts(yearsdf$Entries)
setwd("~/Dropbox/R Code")
source("shift.R")
yearsdf$Exits2<-shift(yearsdf$Exits, -1)
yearsdf$Exits2[is.na(yearsdf$Exits2)]<-0
yearsdf$States.at.War<-cumsum(yearsdf$Entries - yearsdf$Exits2)
rm(endyear)
rm(startyear)
setwd("~/Dropbox/Data General/COW Intra-state Wars")
df2<-read.csv("Intra-StateWarData_v4.1.csv")
### Make a dataframe for # of intrastate war entries each year
startyear.int<-as.data.frame(table(df2$StartYear1))
names(startyear.int)<-c("Year", "Entries.int")
class(startyear.int$Year)
### Merge dataframe for total war entries in all years
yearsdf<-merge(yearsdf, startyear.int, by="Year", all.x=TRUE)
yearsdf$Entries.int<-ifelse(is.na(yearsdf$Entries.int), 0, yearsdf$Entries.int)
### Make a dataframe for # of war exits each year
endyear.int<-as.data.frame(table(df2$EndYear1))
names(endyear.int)<-c("Year", "Exits.int")
### Merge exits with entries
yearsdf<-merge(yearsdf, endyear.int, by="Year", all.x=TRUE)
yearsdf$Exits.int<-ifelse(is.na(yearsdf$Exits.int), 0, yearsdf$Exits.int)
yearsdf$Exits.int<-as.ts(yearsdf$Exits.int)
yearsdf$Entries.int<-as.ts(yearsdf$Entries.int)
yearsdf$Exits2.int<-shift(yearsdf$Exits.int, -1)
yearsdf$Exits2.int[is.na(yearsdf$Exits2.int)]<-0
yearsdf$Wars.Against.States<-cumsum(yearsdf$Entries.int - yearsdf$Exits2.int)
data<-yearsdf
library(TTR)
data$Year<-as.numeric(levels(data$Year))
rm(df, df2, yearsdf, endyear.int,startyear.int)
